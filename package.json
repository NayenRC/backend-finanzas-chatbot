{
  "name": "backend-edu-node24",
  "version": "1.0.0",
  "description": "Proyecto base educativo de Backend con Node 24, Express, Knex y OpenRouter",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "command:seed": "node src/commands/seed.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "keywords": [
    "nodejs",
    "education",
    "express",
    "knex"
  ],
  "author": "Antigravity",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.91.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.3",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "node-telegram-bot-api": "^0.67.0",
    "nodemailer": "^7.0.13",
    "objection": "^3.1.5",
    "pg": "^8.17.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}